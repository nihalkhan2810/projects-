l=[2,5,11,15]
target=int(input("enter a target"))
def sum(l,target):
    for i in range(len(l)-1):
        for j in range(i+1,len(l)):
            if (l[i] + l[j]==target):
                return i,j
    return -1
                
                
    

result=(sum(l,target))
if result!=-1:
    print("the adress is",result)
else:
    print("not found")
        
